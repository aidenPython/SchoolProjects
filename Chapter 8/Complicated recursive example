def raise_to_power(base_val, exponent_val):
    if exponent_val == 0:
        result = base_val
    else:
        result = base_val * (raise_to_power(base_val, exponent_val - 1))

    return result


user_base = int(input("Enter the base val: "))
user_exponent = int(input("Enter the exponeval: "))

user_value = raise_to_power(user_base, user_exponent)

print(user_value)





